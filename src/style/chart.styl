#chart-container
  width 800px
  position absolute
  top 0
  left 0px
  display none

  #island, #mainland
    height 600px

  .show-island
    top 0
    background red

  .show-mainland
    top 140px
    background blue

  .button
    position absolute
    right -50px
    width 50px
    height 140px
    cursor pointer
